{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/monorepo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/monorepo/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/monorepo",
  "projects": {
    "monorepo": {
      "type": "application",
      "root": "apps/monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepo/tsconfig.app.json"
      }
    },
    "nest_admin": {
      "type": "application",
      "root": "apps/nest_admin",
      "entryFile": "main",
      "sourceRoot": "apps/nest_admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest_admin/tsconfig.app.json"
      }
    },
    "nest_landing": {
      "type": "application",
      "root": "apps/nest_landing",
      "entryFile": "main",
      "sourceRoot": "apps/nest_landing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest_landing/tsconfig.app.json"
      }
    },
    "nest_pelamar": {
      "type": "application",
      "root": "apps/nest_pelamar",
      "entryFile": "main",
      "sourceRoot": "apps/nest_pelamar/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest_pelamar/tsconfig.app.json"
      }
    },
    "api_builder": {
      "type": "library",
      "root": "libs/api_builder",
      "entryFile": "index",
      "sourceRoot": "libs/api_builder/src",
      "compilerOptions": {
        "tsConfigPath": "libs/api_builder/tsconfig.lib.json"
      }
    },
    "decrypt": {
      "type": "library",
      "root": "libs/decrypt",
      "entryFile": "index",
      "sourceRoot": "libs/decrypt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decrypt/tsconfig.lib.json"
      }
    },
    "encrypt": {
      "type": "library",
      "root": "libs/encrypt",
      "entryFile": "index",
      "sourceRoot": "libs/encrypt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/encrypt/tsconfig.lib.json"
      }
    },
    "jwt": {
      "type": "library",
      "root": "libs/jwt",
      "entryFile": "index",
      "sourceRoot": "libs/jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt/tsconfig.lib.json"
      }
    }
  }
}